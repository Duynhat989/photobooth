{
  "name": "FesZone",
  "version": "2.1.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron . --dev",
    "build": "electron-builder",
    "dist": "electron-builder --win --x64"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^31.7.7",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "electron-is-dev": "^3.0.1",
    "express": "^4.21.1",
    "node-machine-id": "^1.1.12",
    "npm": "^10.8.2"
  },
  "build": {
    "appId": "com.feszone.changeapp",
    "productName": "ChangeGarena",
    "files": [
      "index.js",
      "package.json",
      "dist/**",
      "src/**",
      "node_modules/**",
      "**/*"
    ],
    "extraResources": [
      {
        "from": "browsers/",
        "to": "browsers",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "data/",
        "to": "data",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
